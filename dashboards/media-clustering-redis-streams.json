{
  "id": null,
  "uid": "media-cluster-observability",
  "title": "Media Clustering â€“ Pipeline Health",
  "tags": ["sploot", "media-clustering", "redis-streams"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Successful Jobs",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "sum(cluster_jobs_total{outcome=\"success\"})",
          "legendFormat": "success",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Failed Jobs",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1, "color": "orange"},
              {"value": 5, "color": "red"}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "sum(cluster_jobs_total{outcome=~\"failure|error\"})",
          "legendFormat": "failure",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 0}
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Last Noise Count",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 20, "color": "orange"},
              {"value": 50, "color": "red"}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "max(cluster_last_noise_count)",
          "legendFormat": "noise",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0}
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Job Throughput",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops/min"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(cluster_jobs_total{outcome=\"success\"}[$__rate_interval]))",
          "legendFormat": "success/min",
          "refId": "A"
        },
        {
          "expr": "sum(rate(cluster_jobs_total{outcome!\u003d\"success\"}[$__rate_interval]))",
          "legendFormat": "failures/min",
          "refId": "B"
        }
      ],
      "gridPos": {"h": 8, "w": 16, "x": 0, "y": 4}
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Clustering Duration (p95)",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 2, "color": "orange"},
              {"value": 5, "color": "red"}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(cluster_job_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(cluster_job_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 4}
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Feedback Operations",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops/min"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(cluster_feedback_operations_total[$__rate_interval])) by (operation, outcome)",
          "legendFormat": "{{operation}} {{outcome}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12}
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Merge Successes",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "sum(cluster_feedback_operations_total{operation=\"merge\", outcome=\"success\"})",
          "legendFormat": "merge",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 4, "w": 12, "x": 0, "y": 20}
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Split Successes",
      "datasource": {
        "type": "prometheus",
        "uid": "${PROM_DS}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "sum(cluster_feedback_operations_total{operation=\"split\", outcome=\"success\"})",
          "legendFormat": "split",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 4, "w": 12, "x": 12, "y": 20}
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "PROM_DS",
        "label": "Prometheus",
        "query": "prometheus",
        "refresh": 1
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"],
    "time_options": ["1h", "6h", "12h", "24h", "48h"]
  }
}
